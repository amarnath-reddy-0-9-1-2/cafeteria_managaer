<table>
  <h3>
    <th colspan="4" class="Header-table"><%= title %></th>
  </h3>
  <tr>
    <th>Id</th><th>Name</th><th>No.of orders</th><th>Orders</th>
  </tr>
  <%persons.each do |person|%>
    <%if person.orders.delivered.count != 0%>
      <tr>
        <td><%=person.id%></td>
        <td><%=person.name%></td>
        <td><%=person.orders.delivered.count%></td>
        <td>
           <% person.orders.delivered.get_orders_between(@start_date,@end_date).ids.each do |id| %>
              <%= link_to id, "orders/#{id}" %>,
           <% end %>
        </td>
      </tr>
    <%end%>
  <%end%>
</table>
